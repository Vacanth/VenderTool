<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.vendertool.inventory.dal.bof.Description" catalog="stool" lazy="false" table="DESCRIPTION">
        <id name="descriptionCode" type="int">
            <column name="DESCRIPTION_CODE" precision="5" scale="0" />
            <!-- <generator class="increment" /> -->
        </id>
        <property name="descriptionValue" type="string">
            <column name="DESCRIPTION_VALUE" length="45" />
        </property>
        <property name="languageCode" type="int">
            <column name="LANGUAGE_CODE" precision="5" scale="0" />
        </property>
         <property name="createDate" type="date">
            <column name="CREATE_DATE"  length="45" />
        </property>
        <property name="modifyDate" type="date">
            <column name="MODIFY_DATE"  length="45" />
        </property>
        <property name="changeWho" type="string">
            <column name="CHANGE_WHO" length="45" not-null="true" />
        </property>
        
        	
	
	
    </class>
</hibernate-mapping>