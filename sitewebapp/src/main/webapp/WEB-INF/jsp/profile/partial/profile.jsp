<table id="info" class="info readonly" cellspacing="0" cellpadding="0">
	<tr>
		<td class="col1"><label>Email:</label></td>
		<td class="col2"><input ng-model="account.emailId" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Address 1:</label></td>
		<td class="col2"><input ng-model="account.contactDetails.address.addressLine1" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Address 2:</label></td>
		<td class="col2"><input ng-model="account.contactDetails.address.addressLine2" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>City:</label></td>
		<td class="col2"><input ng-model="account.contactDetails.address.city" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>State:</label></td>
		<td class="col2"><input ng-model="account.contactDetails.address.state" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Zip:</label></td>
		<td class="col2"><input ng-model="account.contactDetails.address.zip" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Country:</label></td>
		<td class="col2"><input ng-model="account.contactDetails.address.country" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Phone (work):</label></td>
		<td class="col2"><input ng-model="" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Phone (mobile):</label></td>
		<td class="col2"><input ng-model="" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Phone (home):</label></td>
		<td class="col2"><input ng-model="" class="form-control" readonly /></td>
	</tr>
	
</table>