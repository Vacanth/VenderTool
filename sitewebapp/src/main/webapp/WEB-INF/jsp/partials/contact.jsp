<table id="info" class="info readonly" cellspacing="0" cellpadding="0">
	<tr>
		<td class="col1"><label>Email:</label></td>
		<td class="col2"><input ng-model="acctFieldSet.email.value" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Address 1:</label></td>
		<td class="col2"><input ng-model="acctFieldSet.addressLine1.value" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Address 2:</label></td>
		<td class="col2"><input ng-model="acctFieldSet.addressLine2.value" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>City:</label></td>
		<td class="col2"><input ng-model="acctFieldSet.city.value" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>State:</label></td>
		<td class="col2"><input ng-model="acctFieldSet.state.value" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Zip:</label></td>
		<td class="col2"><input ng-model="acctFieldSet.zip.value" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Country:</label></td>
		<td class="col2"><input ng-model="acctFieldSet.country.value" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Phone (work):</label></td>
		<td class="col2"><input ng-model="acctFieldSet.phoneWork.value" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Phone (mobile):</label></td>
		<td class="col2"><input ng-model="acctFieldSet.phoneMobile.value" class="form-control" readonly /></td>
	</tr>
	<tr>
		<td class="col1"><label>Phone (home):</label></td>
		<td class="col2"><input ng-model="acctFieldSet.phoneHome.value" class="form-control" readonly /></td>
	</tr>
	
</table>